{
  "name": "make-the-melon",
  "version": "1.0.0",
  "main": "index.js",
  "homepage": "https://chu-rit.github.io/makeTheMelon",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "expo start --web",
    "build": "expo export --platform web",
    "build-clean": "expo export --platform web && rm -rf clean-build && mkdir clean-build && cp -r dist/* clean-build/ && sed -i 's/_expo\\/static/static/g' clean-build/index.html",
    "deploy": "expo export --platform web && gh-pages -d dist"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.1",
    "expo": "~52.0.23",
    "expo-status-bar": "~2.0.1",
    "phaser": "^3.80.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2"
  },
  "private": true
}
